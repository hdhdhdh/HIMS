<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!--实现的哪个接口呢？ 填入namespace，这个xml是这个接口的实现类-->
<mapper namespace="com.whut.dao.IAdministratorsDao" >

    <select id="getAdministratorsById"  resultType="com.whut.bean.Administrators">
        select *
        from tb_administrators
        where ad_id=#{ad_id}
    </select>
</mapper>
