<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!--实现的哪个接口呢？ 填入namespace，这个xml是这个接口的实现类-->
<mapper namespace="com.whut.dao.ICaseDao" >
    <delete id="deleteCase" parameterType="int">
        delete from tb_case where c_id=#{c_id}
    </delete>
    <insert id="addCase" parameterType="case">
        insert into tb_case(c_id,d_id,p_id,c_description,c_fee,c_status,c_date)
                  value (#{c_id},#{d_id},#{p_id},#{c_description},#{c_fee},#{c_status},#{c_date})
    </insert>
</mapper>
        <!--这个是mapper模板 -->